  "scripts": {
    "ng": "ng", 
    "start": "concurrently --kill-others \"npm run _server:run\" \"ng serve --aot=true --progress=false --proxy-config proxy.config.json\"",
    "build": "ng build --prod --env=prod --sm=false --aot=true && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"mongod\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",

    "postinstall": "npm run build"
  },
